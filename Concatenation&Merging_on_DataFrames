   "source": [
    "# Outer Merge/Outer Join :- It will merge/join the all the common and uncommon rows from both the dataframes considering 'city' as joining column\n",
    "\n",
    "pd.merge(df1,df2,on=\"city\",how=\"outer\")"
   ]
  },
