    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "d1 = {\"city\":['mumbai','hyderabad','pune','bengluru','delhi'],\"temperature\":[30,31,33,43,44]}\n",
    "d2 = {\"city\":['pune','hyderabad','mumbai','noida','chennai'],\"humidity\":[60,61,73,63,74]}\n",
    "\n",
    "\n",
    "df1 = pd.DataFrame(d1)\n",
    "df2 = pd.DataFrame(d2)"
   ]
